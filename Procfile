web: bokeh serve --port=$PORT --host=fpl-playerdata.herokuapp.com --host=* --address=0.0.0.0 --use-xheaders players.py

web: bokeh serve \
    --log-level=debug \
    --num-procs=0 \
    --port=$PORT \
    --show \
    --host=fpl-playerdata.herokuapp.com \
    --host=* --address=0.0.0.0 \
    --use-xheaders players.py